{% extends 'template_coreui.html.twig' %}


{% block title %}{{ page_title }}{% endblock %}

{% block home_path %}
<li class="breadcrumb-item">    
    {% if not page_title %}
        Configuration
    {% else %}
        <a href="#">Configuration</a>
    {% endif %}
</li>
{% if page_title %}
<li class="breadcrumb-item">
    {{ page_title }}
</li>
{% endif %}
{% endblock %}

{% block body_header_right %}
<a href="{{ create_url }}" class="btn"><i class="fa fa-plus"></i></a>
{% endblock %}


{% block body %}

<div class="example-wrapper">
    <div class="animated fadeIn">
        <div class="row">
            <div class="col-lg-12">
                <div class="card">
                    <div class="card-header">
                        <i class="fa fa-align-justify"></i> {{ page_title | default('Configuration') }}</div>
                    <div class="card-body">                       

                        {% include page %}

                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
{% endblock %}

{% block javascripts %} 
<script src="{{ asset('setting.js','qobd_web_js') }}"></script>
{% endblock %}
